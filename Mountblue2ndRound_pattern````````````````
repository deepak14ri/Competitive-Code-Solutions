

n = 4
for i in range(n*2-1):
    for j in range(2*n-2*i-1):
        if i==3:
            for k in range(1,5):
                for l in range(2*k-1):
                    print(k,end='')
                print()
        else:
            print(5-i-1,end='')
    print()

